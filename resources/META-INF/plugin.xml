<idea-plugin>
    <id>com.emonadeo.tanne</id>
    <name>Tanne Theme</name>
    <version>0.2.0</version>
    <vendor url="https://github.com/emonadeo">Emonadeo</vendor>

    <description>
        <![CDATA[
            Eye comforting dark green theme for JetBrains IDEs.
            <br/><br/>
        ]]>
    </description>

    <change-notes>
        <![CDATA[
            <h3>0.2.0</h3>
            <ul>
                <li>Fixed supported IDEA version</li>
                <li>Swapped default text with identifier color</li>
                <li>Improved brightness of #99BDB5 and #8AA8A2 to #A8C7C0</li>
            </ul>
        ]]>
    </change-notes>

    <idea-version since-build="191.0"/>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <themeProvider id="3ee24c2b-dfef-4bee-b37e-18bd2864ffeb" path="/tanne.theme.json"/>
    </extensions>
</idea-plugin>
